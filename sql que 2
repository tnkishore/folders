	distinct sc.camp_id,
	sc.per_id as contact_id,
	veh.asset_num as vin,
	sc.asset_id,
	date (sc.prog_start_dt) as prog_start_dt,
	date(sc.prog_end_dt) as prog_end_dt,
	sc.campaign_name as camp_name,
