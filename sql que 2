select
	a.asset_num,
	count(*) as row_cnt,
	count(a.contact_id) as cont_id,
	count(distinct a.contact_id) as dist_cont_id,
	count(a.phone) as phone,
	count(distinct a.phone) dist_phone,
	count(a.email) email,
	count(distinct a.email) as dist_email
from
	gcrm.cust_seg2 as a
group by
	1;

